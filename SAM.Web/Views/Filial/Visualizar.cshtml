@model SAM.Web.ViewModels.Filial.FilialVisualizarViewModel

@{
    ViewBag.Title = "Visualizar";
    Layout = "~/Views/Shared/_LayoutLogado.cshtml";
}


<div class="forms columnar 1">

    <h4>Unidade - @Model.NomeFantasiaEmpresa</h4>
    <fieldset>
        <ul>
            <li>
                <fieldset>
                    <section>
                        <label for="" class="bold">Ativo</label>
                    </section>
                    <input type="checkbox" name="Ativo" @(@Model.Ativo ? "checked" : "") disabled="disabled" value="true">
                </fieldset>
            </li>

            <li>
                @Html.LabelFor(model => model.RazaoSocial)
                @Html.DisplayFor(model => model.RazaoSocial)&nbsp;               
            </li>

            <li>
                @Html.LabelFor(model => model.NomeFantasia)
                @Html.DisplayFor(model => model.NomeFantasia)&nbsp;
            </li>
            <li>
                @Html.LabelFor(model => model.Cnpj)
                @Html.DisplayFor(model => model.Cnpj)&nbsp;
            </li>

            <li>
                @Html.LabelFor(model => model.Telefone1)
                @Html.DisplayFor(model => model.Telefone1)&nbsp;
            </li>
            <li>
                @Html.LabelFor(model => model.Telefone2)
                @Html.DisplayFor(model => model.Telefone2)&nbsp;
            </li>
        </ul>

    </fieldset>

    <ul>
        <li class="push">
            <a href="/Filial/Cadastrar/@Model.IdFilial">
                <button class="btn">Editar</button></a>
            <input type="button" name="send" class="btn" value="Voltar" onclick="Voltar();" />
        </li>
    </ul>
</div>
